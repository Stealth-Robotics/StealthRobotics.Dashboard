<Window x:Class="StealthRobotics.Dashboard.Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StealthRobotics.Dashboard.Launcher"
        mc:Ignorable="d"
        Title="Dashboard launcher" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <StackPanel.Resources>
            <Style TargetType="Button">
                <Setter Property="Margin" Value="0 0 0 2"/>
                <Setter Property="Padding" Value="2"/>
            </Style>
        </StackPanel.Resources>
        <Button Content="WPF Dashboard" Name="wpf" Click="Wpf_Click"/>
        <Button Content="Java Dashboard" Name="java" Click="Java_Click"/>
        <Button Content="Shuffleboard" Name="jfx" Click="Jfx_Click"/>
        <Button Content="Outline Viewer" Name="ov" Click="Ov_Click"/>
    </StackPanel>
</Window>
